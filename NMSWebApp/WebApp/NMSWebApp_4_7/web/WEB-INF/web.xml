<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>servlets.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>servlets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>About</servlet-name>
        <servlet-class>servlets.About</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Configure</servlet-name>
        <servlet-class>servlets.Configure</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfigureHost</servlet-name>
        <servlet-class>servlets.ConfigureHost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfigureService</servlet-name>
        <servlet-class>servlets.ConfigureService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfigureMatch</servlet-name>
        <servlet-class>servlets.ConfigureMatch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfigureSettings</servlet-name>
        <servlet-class>servlets.ConfigureSettings</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Monitor</servlet-name>
        <servlet-class>servlets.Monitor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonitorHost</servlet-name>
        <servlet-class>servlets.MonitorHost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonitorService</servlet-name>
        <servlet-class>servlets.MonitorService</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>About</servlet-name>
        <url-pattern>/About</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Configure</servlet-name>
        <url-pattern>/Configure</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfigureHost</servlet-name>
        <url-pattern>/ConfigureHost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfigureService</servlet-name>
        <url-pattern>/ConfigureService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfigureMatch</servlet-name>
        <url-pattern>/ConfigureMatch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfigureSettings</servlet-name>
        <url-pattern>/ConfigureSettings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Monitor</servlet-name>
        <url-pattern>/Monitor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MonitorHost</servlet-name>
        <url-pattern>/MonitorHost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MonitorService</servlet-name>
        <url-pattern>/MonitorService</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
